      <!------FIN NAVBAR-->
            <header class="header-sticky header-dark">
              <nav class="navbar navbar-expand-lg navbar-light bg-light">
              <a class="navbar-brand" href="#">
                <img src="../../assets/demo/logo/nectar-dark5.png"" width="80" height="40" class="d-inline-block align-top" alt="">
              </a>
              <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>

              <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                  <li class="nav-item active">
                    <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
                  </li>
                  <li class="nav-item">
                    <a class="nav-link" href="<%=artworks_path%>">Collections</a>
                  </li>
                </ul>
              </div>
                <% if user_signed_in?%>
              <ul class="navbar-nav align-items-center mr-0">
                  <li class="nav-item">
                      <a class="nav-link", role="button">
                          <%= link_to "Sign out", destroy_user_session_path , :method => :delete  %>
                      </a>
                  </li> 

                <li class="nav-item">
                  <a class="nav-link", role="button">
                    <%= link_to "My profile", user_session_path %>
                      </a>
                    </li>

               <li class="nav-item">
                <a class="nav-link", role="button">
                  <%= link_to "Cart", carts_path %>
                </a>
              </li>
              </ul>
              <%else%>
              <ul class="navbar-nav align-items-center mr-0">
                <li class="nav-item">
                  <a class="nav-link" , role="button">
                    <%= link_to "Sign in", user_session_path %>
                  </a>
                </li>

                <li class="nav-item">
                  <a class="nav-link" , role="button">
                    <%= link_to "Sign Up", new_user_path %>
                  </a>
                </li>

              </ul>
              <%end%>

</nav>
</header>
