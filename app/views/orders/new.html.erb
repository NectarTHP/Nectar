<h1>Checkout</h1>
<p>Confirm shipping address details
<%= form_for(@order) do |f| %>
  <p>
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name, required: true, autofocus: true %>
  </p>
    <p>
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name, required: true, autofocus: true %>
  </p>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label :adress %><br>
    <%= f.text_area :adress %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>%